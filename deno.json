{
  "tasks": {
    "start": "deno run --allow-net --allow-env --env-file src/main.ts",
    "serve": "deno run --watch --allow-net --allow-env --unstable-kv --env-file src/serve.tsx",
    "create-book-store": "deno run --allow-net --allow-read=book-pr.md --allow-env --env-file src/create-book-store.ts"
  },
  "imports": {
    "@deno/gfm": "jsr:@deno/gfm@^0.10.0",
    "@langchain/community": "npm:@langchain/community@^0.3.16",
    "@langchain/core": "npm:@langchain/core@^0.3.19",
    "@langchain/langgraph": "npm:@langchain/langgraph@^0.2.23",
    "@langchain/openai": "npm:@langchain/openai@^0.3.14",
    "@neondatabase/serverless": "npm:@neondatabase/serverless@^0.10.4",
    "@std/assert": "jsr:@std/assert@1",
    "hono": "npm:hono@^4.6.12",
    "langchain": "npm:langchain@^0.3.6",
    "octokit": "npm:octokit@4.0.2",
    "ts-pattern": "npm:ts-pattern@^5.5.0",
    "zod": "npm:zod@^3.23.8"
  },
  "fmt": {
    "singleQuote": true,
    "semiColons": false
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
